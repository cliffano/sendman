{
  "commands": {
    "init": {
      "desc": "Create example .sendman.json configuration file",
      "examples": [
        "sendman init"
      ]
    },
    "send": {
      "desc": "Send file(s)",
      "options": [
        { "arg": "-f, --file <file>", "desc": "Configuration file | default: .sendman.json" }
      ],
      "examples": [
        "sendman send --file .somesendman.json"
      ]
    }
  }
}
